{% load static %}<!DOCTYPE html>
<html lang="{{ LANGUAGE_CODE|default:"en-us" }}" {% if LANGUAGE_BIDI %}dir="rtl"{% endif %} xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>{{ bots_environment_text }} {{ bots_http_path }}</title>
    <link rel="stylesheet" type="text/css" href="{% block stylesheet %}{% static "admin/css/base.css" %}{% endblock %}" />
    {% block extrastyle %}{% endblock %}
    <!--[if lte IE 7]><link rel="stylesheet" type="text/css" href="{% block stylesheet_ie %}{{ STATIC_URL }}css/ie.css{% endblock %}" /><![endif]-->
    {% block extrahead %}{% endblock %}
    {% if LANGUAGE_BIDI %}<link rel="stylesheet" type="text/css" href="{% block stylesheet_rtl %}{{ STATIC_URL }}css/rtl.css{% endblock %}" />{% endif %}
    <script type="text/javascript">window.__admin_media_prefix__ = "{% filter escapejs %}{{ STATIC_URL }}{% endfilter %}";</script>
    <link rel="icon" type="image/png" href="/media/images/favicon.ico"/>
    <link rel="stylesheet" type="text/css" href="/media/css/bots.css" />
</head>
{% load i18n %}


<body class="{% if is_popup %}popup {% endif %}{% block bodyclass %}{% endblock %}">
{% if not is_popup %}
    <!-- Fixed botslogo include -->
    <div id="logo">
        <a href="/home/"><img src="/media/images/botslogo.png" /></a>
    </div>
    <div id="environment">
        <div style="color: {{ bots_environment_text_color }}">{{ bots_environment_text }}</div>
    </div>
    {% if user.is_authenticated %}
        <div id="user-tools">
        <div id="navi">
                {% if user.is_active %}
                    <a href="/myweb/profile/">{{ user.username }}</a>
                {% endif %}
                {% if "admin_site" in request.META.PATH_INFO %}
                    <a href="{% url "admin:logout" %}?next=/login/">{% trans 'Log out' %}</a>
                {% else %}
                    <a href="/logout/">{% trans 'Log out' %}</a>
                {% endif %}
                {% if user.is_staff or user.is_superuser %}
                    <a href="/admin/">{% trans 'Admin' %}</a>
                {% endif %}
        </div>
        </div>
    {% endif %}

    {% if user.is_authenticated %}
        <div id="menu">
            <ul>
                <li><a href="/home/">{% trans 'Home' %}</a></li>
                <li><a href="#">{% trans 'Run' %}</a>
                    <ul>
                        <li><a href="/run/">{% trans 'Run engine' %}</a></li>
                        {% if menu_all_routes %}
                            <li><a href="#">{% trans 'Run route' %}</a>
                                <ul>
                                    {% for route in menu_all_routes %}
                                        <li><a href="/runroute/?idroute={{ route }}">{{ route }}</a></li>
                                    {% endfor %}
                                </ul>
                            </li>
                        {% endif %}
                        {% if user.is_staff or menu_cleanup %}
                            <li><a href="/cleanup/">{% trans 'Clean up' %}</a></li>
                        {% endif %}
                        <li><a href="/engine-report/">{% trans 'Recent engine runs' %}</a></li>
                    </ul>
                </li>
                <li><a href="#">{% trans 'Reports' %}</a>
                    <ul>
                        <li><a href="/reports/">{% trans 'All reports' %}</a></li>
                        <li><a href="/incoming/?lastrun=1">{% trans 'Last run' %}</a></li>
                        <li><a href="/document/?lastrun=1">{% trans 'Last run per document' %}</a></li>
                        <li><a href="/process/?lastrun=1">{% trans 'Process errors' %}</a></li>
                    </ul>
                </li>
                <li><a href="#">{% trans 'Configuration' %}</a>
                    <ul>
                        <li><a href="/routes/?all=0">{% trans 'Routes' %}</a></li>
                        <li><a href="/channels/?all=0">{% trans 'Channels' %}</a></li>
                        <li><a href="/partners/?all=0">{% trans 'Partners' %}</a></li>
                        <li><a href="/translate/">{% trans 'Translations' %}</a></li>
                        <li><a href="/user/">{% trans 'Users' %}</a></li>
                    </ul>
                </li>
                {% if custom_menus %}
                    <li><a href="#">{{ custom_menuname }}</a>
                        <ul>
                            {% for menuname, menuurl in custom_menus %}
                                <li><a href="{{ menuurl }}">{{ menuname }}</a></li>
                            {% endfor %}
                        </ul>
                    </li>
                {% endif %}
                <li><a href="#">{% trans 'Utilities' %}</a>
                    <ul>
                        <li><a href="/plugin/">{% trans 'Plugins' %}</a></li>
                        <li><a href="/delete/">{% trans 'Delete messages' %}</a></li>
                        <li><a href="/confirm/">{% trans 'Confirm messages by user' %}</a></li>
                        <li><a href="/filer/">{% trans 'Filer' %}</a></li>
                        {% if user.is_staff or menu_automaticretrycommunication %}
                            <li><a href="/retry/">{% trans 'Retry communication errors' %}</a></li>
                        {% endif %}
                    </ul>
                </li>
                <li><a href="#">{% trans 'Help' %}</a>
                    <ul>
                        <li><a href="https://bots.readthedocs.io" target="_blank">{% trans 'On-line manual' %}</a></li>
                        <li><a href="/help/?botshelp=about">{% trans 'About Bots' %}</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    {% endif %}
{% endif %}

{% if messages %}
    <div class="messages">
    {% for message in messages %}
        <div {% if message.tags %} class="{{ message.tags }}"{% endif %}>{{ message }}</div>
    {% endfor %}
    </div>
{% endif %}

<!-- Container -->
<div id="container">
    <!-- Content -->
    <div id="content" style="display: block;">
        {% block content %}
            {% block object-tools %}{% endblock %}
            {{ content }}
        {% endblock %}
    </div>
    <!-- END Content -->
</div>
<!-- END Container -->

</body>
</html> 